        linkml:ClassDefinition ;
    rdfs:label "Delete" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor has deleted the object. If specified, the origin indicates the context from which the object was deleted." ;
    skos:exactMatch as:Delete ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

