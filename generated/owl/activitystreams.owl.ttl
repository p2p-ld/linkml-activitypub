@prefix as: <http://www.w3.org/ns/activitystreams#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix unit: <http://qudt.org/2.1/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

as:Property a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Property" ;
    skos:exactMatch rdf:Property ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Statement a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Statement" ;
    skos:exactMatch rdf:Statement ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:closed a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "closed" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link linkml:Datetime linkml:Boolean ) ] ;
    skos:definition "Indicates that a question has been closed, and answers are no longer accepted." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "type" ;
    rdfs:range as:anyURI ;
    skos:definition "Identifies the Object or Link type. Multiple values may be specified." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Add a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Add" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor has added the object to the target. If the target property is not explicitly specified, the target would need to be determined implicitly by context. The origin can be used to identify the context from which the object originated." ;
    skos:exactMatch as:Add ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Announce a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Announce" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor is calling the target's attention the object. The origin typically has no defined meaning." ;
    skos:exactMatch as:Announce ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Application a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Application" ;
    rdfs:subClassOf as:Object ;
    skos:definition "Describes a software application." ;
    skos:exactMatch as:Application ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Arrive a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Arrive" ;
    rdfs:subClassOf as:IntransitiveActivity ;
    skos:definition "An IntransitiveActivity that indicates that the actor has arrived at the location. The origin can be used to identify the context from which the actor originated. The target typically has no defined meaning." ;
    skos:exactMatch as:Arrive ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Article a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Article" ;
    rdfs:subClassOf as:Object ;
    skos:exactMatch as:Article ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Audio a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Audio" ;
    rdfs:subClassOf as:Document ;
    skos:definition "Represents an audio document of any kind." ;
    skos:exactMatch as:Audio ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Block a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Block" ;
    rdfs:subClassOf as:Ignore ;
    skos:definition "Indicates that the actor is blocking the object. Blocking is a stronger form of Ignore. The typical use is to support social systems that allow one user to block activities or content of other users. The target and origin typically have no defined meaning." ;
    skos:exactMatch as:Block ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Create a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Create" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor has created the object." ;
