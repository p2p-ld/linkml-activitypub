as:Accept a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Accept" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor accepts the object. The target property can be used in certain circumstances to indicate the context into which the object has been accepted." ;
    skos:exactMatch as:Accept ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Ignore a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Ignore" ;
