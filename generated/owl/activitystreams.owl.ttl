        linkml:ClassDefinition ;
    rdfs:label "Reject" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "Indicates that the actor is rejecting the object. The target and origin typically have no defined meaning." ;
    skos:exactMatch as:Reject ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:actor a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "actor" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    rdfs:subPropertyOf as:attributedTo ;
    skos:definition "Describes one or more entities that either performed or are expected to perform the activity. Any single activity can have multiple actors. The actor MAY be specified using an indirect Link." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:anyOf a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "anyOf" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies an inclusive option for a Question. Use of anyOf implies that the Question can have multiple answers. To indicate that a Question can have only one answer, use oneOf." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:attachment a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "attachment" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies a resource attached or related to an object that potentially requires special handling. The intent is to provide a model that is at least semantically similar to attachments in email." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:audience a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "audience" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies one or more entities that represent the total population of entities for which the object can considered to be relevant." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:bcc a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "bcc" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies one or more Objects that are part of the private secondary audience of this Object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:bto a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "bto" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies an Object that is part of the private primary audience of this Object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:cc a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "cc" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies an Object that is part of the public secondary audience of this Object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:content a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "content" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:unionOf ( linkml:String as:langString ) ] ;
    skos:definition "The content or textual representation of the Object encoded as a JSON string. By default, the value of content is HTML. The mediaType property can be used in the object to indicate a different content type. The content MAY be expressed using multiple language-tagged values." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:context a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "context" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition """Identifies the context within which the object exists or an activity was performed.
The notion of "context" used is intentionally vague. The intended function is to serve as a means of grouping objects and activities that share a common originating context or purpose. An example could be all activities relating to a common project or event.
""" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:durationType a owl:Class,
        linkml:TypeDefinition ;
    rdfs:subClassOf linkml:String .

as:formerType a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "formerType" ;
    rdfs:range as:Object ;
    skos:definition "On a Tombstone object, the formerType property identifies the type of the object that was deleted." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:generator a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "generator" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies the entity (e.g. an application) that generated the object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:icon a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "icon" ;
    rdfs:range [ owl:unionOf ( as:Image as:Link ) ] ;
    skos:definition "Indicates an entity that describes an icon for this object. The image should have an aspect ratio of one (horizontal) to one (vertical) and should be suitable for presentation at a small size." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:image a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "image" ;
    rdfs:range [ owl:unionOf ( as:Image as:Link ) ] ;
    skos:definition "Indicates an entity that describes an image for this object. Unlike the icon property, there are no aspect ratio or display size limitations assumed." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:inReplyTo a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "inReplyTo" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Indicates one or more entities for which this object is considered a response." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:instrument a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "instrument" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies one or more objects used (or to be used) in the completion of an Activity." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:location a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "location" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Indicates one or more physical or logical locations associated with the object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:oneOf a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "oneOf" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies an exclusive option for a Question. Use of oneOf implies that the Question can have only a single answer. To indicate that a Question can have multiple answers, use anyOf." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:origin a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "origin" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Describes an indirect object of the activity from which the activity is directed. The precise meaning of the origin is the object of the English preposition \"from\". For instance, in the activity \"John moved an item to List B from List A\", the origin of the activity is \"List A\"." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:rel a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "rel" ;
    rdfs:range linkml:String ;
    skos:definition """A link relation associated with a Link. The value MUST conform to both the [HTML5] and [RFC5988] "link relation" definitions.
In the [HTML5], any string not containing the "space" U+0020, "tab" (U+0009), "LF" (U+000A), "FF" (U+000C), "CR" (U+000D) or "," (U+002C) characters can be used as a valid link relation.
""" ;
    skos:editorialNote "No validation is done or type could be found for declaring the RCF5988 or HTML5 link relation types" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:relationship a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "relationship" ;
    rdfs:range as:Object ;
    skos:definition "On a Relationship object, the relationship property identifies the kind of relationship that exists between subject and object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:result a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "result" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Describes the result of the activity. For instance, if a particular action results in the creation of a new resource, the result property can be used to describe that new resource." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:subject a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "subject" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "On a Relationship object, the subject property identifies one of the connected individuals. For instance, for a Relationship object describing \"John is related to Sally\", subject would refer to John." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:summary a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "summary" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:unionOf ( linkml:String as:langString ) ] ;
    skos:definition "A natural language summarization of the object encoded as HTML. Multiple language tagged summaries MAY be provided." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:tag a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "tag" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "One or more \"tags\" that have been associated with an objects. A tag can be any kind of Object. The key difference between attachment and tag is that the former implies association by inclusion, while the latter implies associated by reference." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:target a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "target" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Describes the indirect object, or target, of the activity. The precise meaning of the target is largely dependent on the type of action being described but will often be the object of the English preposition \"to\". For instance, in the activity \"John added a movie to his wishlist\", the target of the activity is John's wishlist. An activity can have more than one target." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:to a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "to" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies an entity considered to be part of the public primary audience of an Object" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:url a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "url" ;
    rdfs:range [ owl:unionOf ( as:anyURI as:Link ) ] ;
    skos:definition "Identifies one or more links to representations of the object" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:accuracy a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "accuracy" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( linkml:Float [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:maxInclusive 100 ] ) ] ) ] ;
    skos:definition "Indicates the accuracy of position coordinates on a Place objects. Expressed in properties of percentage. e.g. \"94.0\" means \"94.0% accurate\"." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:altitude a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "altitude" ;
    rdfs:range linkml:Float ;
    skos:definition "Indicates the altitude of a place. The measurement units is indicated using the units property. If units is not specified, the default is assumed to be \"m\" indicating meters." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:attributedTo a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "attributedTo" ;
    rdfs:range [ owl:unionOf ( as:Link as:Object ) ] ;
    skos:definition "Identifies one or more entities to which this object is attributed. The attributed entities might not be Actors. For instance, an object might be attributed to the completion of another activity." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:current a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "current" ;
    rdfs:range [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
    skos:definition "In a paged Collection, indicates the page that contains the most recently updated member items." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:deleted a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "deleted" ;
    rdfs:range linkml:Datetime ;
    skos:definition "On a Tombstone object, the deleted property is a timestamp for when the object was deleted." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:describes a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "describes" ;
    rdfs:range as:Object ;
    skos:definition "On a Profile object, the describes property identifies the object described by the Profile." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:duration a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "duration" ;
    rdfs:range as:durationType ;
    rdfs:seeAlso <https://www.w3.org/TR/xmlschema11-2/> ;
    skos:definition "When the object describes a time-bound resource, such as an audio or video, a meeting, etc, the duration property indicates the object's approximate duration. The value MUST be expressed as an xsd:duration as defined by [ xmlschema11-2], section 3.3.6 (e.g. a period of 5 seconds is represented as \"PT5S\")." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:endTime a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "endTime" ;
    rdfs:range linkml:Datetime ;
    skos:definition "The date and time describing the actual or expected ending time of the object. When used with an Activity object, for instance, the endTime property specifies the moment the activity concluded or is expected to conclude." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:first a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "first" ;
    rdfs:range [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
    skos:definition "In a paged Collection, indicates the furthest preceeding page of items in the collection." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:height a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "height" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( as:nonNegativeInteger [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
    skos:definition "On a Link, specifies a hint as to the rendering height in device-independent pixels of the linked resource." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:href a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "href" ;
    rdfs:range as:anyURI ;
    skos:definition "The target resource pointed to by a Link." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:hreflang a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "hreflang" ;
    rdfs:range linkml:String ;
    skos:closeMatch <http://schema.org/inLanguage> ;
    skos:definition "Hints as to the language used by the target resource. Value MUST be a [BCP47] Language-Tag." ;
    skos:editorialNote "Inheriting from schema:inLanguage in order to specify IETF BCP47 language codes since there isn't a formal ontology I can find" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:last a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "last" ;
    rdfs:range [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
    skos:definition "In a paged Collection, indicates the furthest proceeding page of the collection." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:latitude a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "latitude" ;
    rdfs:range linkml:Float ;
    skos:definition "The latitude of a place" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:longitude a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "longitude" ;
    rdfs:range linkml:Float ;
    skos:definition "The longitude of a place" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:published a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "published" ;
    rdfs:range linkml:Datetime ;
    skos:definition "The date and time at which the object was published" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:radius a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "radius" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( linkml:Float [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
    skos:definition "The radius from the given latitude and longitude for a Place. The units is expressed by the units property. If units is not specified, the default is assumed to be \"m\" indicating \"meters\"." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:replies a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "replies" ;
    rdfs:range as:Collection ;
    skos:definition "Identifies a Collection containing objects considered to be responses to this object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:startIndex a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "startIndex" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( as:nonNegativeInteger [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
    skos:definition "A non-negative integer value identifying the relative position within the logical view of a strictly ordered collection." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:startTime a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "startTime" ;
    rdfs:range linkml:Datetime ;
    skos:definition "The date and time describing the actual or expected starting time of the object. When used with an Activity object, for instance, the startTime property specifies the moment the activity began or is scheduled to begin." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:totalItems a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "totalItems" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( as:nonNegativeInteger [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
    skos:definition "A non-negative integer specifying the total number of objects contained by the logical view of the collection. This number might not reflect the actual number of items serialized within the Collection object instance." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:units a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "units" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:unionOf ( as:UnitEnum as:anyURI ) ] ;
    skos:definition "Specifies the measurement units for the radius and altitude properties on a Place object. If not specified, the default is assumed to be \"m\" for \"meters\"." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:updated a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "updated" ;
    rdfs:range linkml:Datetime ;
    skos:definition "The date and time at which the object was updated" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:width a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "width" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:intersectionOf ( as:nonNegativeInteger [ a rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minInclusive 0 ] ) ] ) ] ;
    skos:definition "On a Link, specifies a hint as to the rendering width in device-independent pixels of the linked resource." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Document a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Document" ;
    rdfs:subClassOf as:Object ;
    skos:definition "Represents a document of any kind." ;
    skos:exactMatch as:Document ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:IntransitiveActivity a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "IntransitiveActivity" ;
    rdfs:subClassOf as:Activity ;
    skos:definition "['An Activity that has no direct object@en']" ;
    skos:exactMatch as:IntransitiveActivity ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:unionOf ( linkml:String as:langString ) ] ;
    skos:definition "A simple, human-readable, plain-text name for the object. HTML markup MUST NOT be included. The name MAY be expressed using multiple language-tagged values." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:object a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "object" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition """When used within an Activity, describes the direct object of the activity. For instance, in the activity "John added a movie to his wishlist", the object of the activity is the movie added.
When used within a Relationship describes the entity to which the subject is related.
""" ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:preview a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "preview" ;
    rdfs:range [ owl:unionOf ( as:Link as:Object ) ] ;
    skos:definition "Identifies an entity that provides a preview of this object." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:Image a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Image" ;
    rdfs:subClassOf as:Document ;
    skos:definition "An image document of any kind" ;
    skos:exactMatch as:Image ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range as:anyURI ;
    skos:definition "Provides the globally unique identifier for an Object or Link." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:items a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "items" ;
    rdfs:range [ owl:unionOf ( as:Object as:Link ) ] ;
    skos:definition "Identifies the items contained in a collection. The items might be ordered or unordered." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:mediaType a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "mediaType" ;
    rdfs:range linkml:String ;
    skos:definition """When used on a Link, identifies the MIME media type of the referenced resource.
When used on an Object, identifies the MIME media type of the value of the content property. If not specified, the content property is assumed to contain text/html content.
""" ;
    skos:editorialNote "Like hreflang, casting as schema:encodingFormat to specify a MIME type" ;
    skos:exactMatch <http://schema.org/encodingFormat> ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:next a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "next" ;
    rdfs:range [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
    skos:definition "In a paged Collection, indicates the next page of items." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:partOf a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "partOf" ;
    rdfs:range [ owl:unionOf ( as:Link as:Collection ) ] ;
    skos:definition "Identifies the Collection to which a CollectionPage objects items belong." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:prev a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "prev" ;
    rdfs:range [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
    skos:definition "In a paged Collection, identifies the previous page of items." ;
    skos:inScheme <https://github.com/p2p_ld/linkml-activitypub> .

as:langString a owl:Class,
        linkml:TypeDefinition ;
    rdfs:subClassOf linkml:String .

as:Collection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Collection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty as:items ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty as:totalItems ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty as:current ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty as:current ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty as:last ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ owl:unionOf ( as:CollectionPage as:OrderedCollectionPage as:Link ) ] ;
            owl:onProperty as:last ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty as:first ],
        [ a owl:Restriction ;
