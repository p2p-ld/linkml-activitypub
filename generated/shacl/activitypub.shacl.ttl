rdf:Property a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass rdf:Property .

rdf:Statement a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:targetClass rdf:Statement .

as:Accept a sh:NodeShape ;
