      "mappings": [
        "http://www.w3.org/ns/activitystreams#audience"
      ],
      "slot_uri": "http://www.w3.org/ns/activitystreams#audience",
      "multivalued": true,
      "owner": "Object",
      "domain_of": [
        "Object"
      ],
      "range": "string",
      "any_of": [
        {
          "range": "Object",
          "@type": "AnonymousSlotExpression"
        },
        {
          "range": "Link",
          "@type": "AnonymousSlotExpression"
        }
      ],
      "@type": "SlotDefinition"
    },
    {
